class Singleton {
public static final Singleton singleton = null;

static Singleton getInstance(){
if(singleton == null) {
return new Singleton();
}
}
}


class Singleton {
public static final Singleton singleton = null;

static Synchronozed Singleton getInstance(){
if(singleton == null) {
return new Singleton();
}
}
}

class Singleton {
public static final Singleton singleton = null;

static Synchronozed Singleton getInstance(){
if(singleton == null) {
sysnchronized(Singleton.class) {
if(singleton ==null){
return new Singleton();
}
}
}
}
}

