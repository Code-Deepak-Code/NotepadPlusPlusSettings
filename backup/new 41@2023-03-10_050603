In mysql console:
DROP USER 'root'@'192.168.29.200';
CREATE USER 'root'@'%' IDENTIFIED BY 'Admin@12345';
GRANT ALL PRIVILEGES ON *.* TO 'root'@'%' WITH GRANT OPTION;
FLUSH PRIVILEGES;

ALTER USER 'dracil'@'192.168.29.200' IDENTIFIED WITH mysql_native_password BY 'Admin@12345';

CREATE USER 'dracil'@'192.168.29.200' IDENTIFIED WITH authentication_plugin BY 'Admin@12345';

UPDATE mysql.user SET Host='%' WHERE Host='localhost' AND User='username';

UPDATE mysql.user SET Host='localhost' WHERE User='root';

UPDATE mysql.db SET Host='192.168.29.200' WHERE Host='192.168.29.200' AND User='root';

docker pull container-registry.oracle.com/database/express:latest


